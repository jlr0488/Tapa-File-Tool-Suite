<Window x:Class="K1_Parser_v1.Views.ProgressWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:K1_Parser_v1.Views"
        mc:Ignorable="d"
        Title="K1 Processing Progress" Height="150" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="10"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="1" Height="25" Name="ProgressTextBlock" Margin="50,10,50,10" VerticalAlignment="Center" HorizontalAlignment="Center" TextAlignment="Center" Text="Starting K1 processing..."/>
        <ProgressBar Grid.Row="2" Name="ProgressBar" Height="25" Margin="50,5,50,20" VerticalAlignment="Top"/>

    </Grid>
</Window>
